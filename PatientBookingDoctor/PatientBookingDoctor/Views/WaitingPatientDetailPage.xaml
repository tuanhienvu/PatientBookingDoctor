<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PatientBookingDoctor.Views.WaitingPatientDetailPage"
             Title="{Binding Title}">

    <StackLayout Spacing="10" Padding="15">
        <Label Text="Tên Bệnh nhân:" FontSize="Medium" />
        <Label Text="{Binding patientName}" FontSize="Small"/>
        <Label Text="Ngày Đăng ký:" FontSize="Medium" />
        <Label Text="{Binding registrationDate}" FontSize="Small"/>
    </StackLayout>

</ContentPage>